{
  "name": "@aisonren/farrow-session",
  "version": "1.0.0",
  "description": "A flexible and decoupled session middleware for Farrow framework",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "clean": "rimraf dist",
    "prebuild": "pnpm clean",
    "prepublishOnly": "pnpm build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "prepare": "husky",
    "commit": "cz"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "keywords": [
    "farrow",
    "session",
    "middleware",
    "cookie",
    "typescript"
  ],
  "author": "Aison",
  "license": "MIT",
  "peerDependencies": {
    "farrow-http": "^2.2.0",
    "farrow-pipeline": "^2.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@jest/globals": "^29.7.0",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/cookies": "^0.9.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.10.6",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "commitizen": "^4.3.1",
    "conventional-changelog": "^6.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prefer-let": "^1.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.3.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.30.1",
    "rollup-plugin-dts": "^6.1.1",
    "standard-version": "^9.5.0",
    "supertest": "^6.1.6",
    "ts-jest": "^29.1.3",
    "tslib": "^2.8.1",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "ulid": "^2.3.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/AisonSu/farrow-session#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AisonSu/farrow-session.git"
  }
}
